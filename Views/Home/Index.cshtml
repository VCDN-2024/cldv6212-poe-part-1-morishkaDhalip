@{
    ViewBag.Title = "Welcome to ABC Retail";
}

<style>
    .bg-beige {
        background-color: #f5f5dc; /* Beige color */
    }

    .text-dark-green {
        color: #004d00; /* Dark Green color */
    }

    .btn-dark-green {
        background-color: #004d00; /* Dark Green color */
        color: #fff;
    }

        .btn-dark-green:hover {
            background-color: #003300; /* Darker Green on hover */
            color: #fff;
        }

    .bg-dark-green {
        background-color: #004d00; /* Dark Green background */
    }

    .shadow-custom {
        box-shadow: 0 4px 8px rgba(0, 0, 0, 0.2); /* Custom shadow */
    }
</style>

<div class="container mt-5">
    <!-- Hero Section -->
    <div class="jumbotron text-center bg-beige text-dark-green shadow-custom">
        <h1 class="display-4">Welcome to ABC Retail</h1>
        <p class="lead">Your one-stop shop for all your retail needs. Discover a wide range of products at unbeatable prices.</p>
        <a href="#about-us" class="btn btn-dark-green btn-lg">Learn More</a>
    </div>

    <!-- About Section -->
    <div id="about-us" class="row mt-5">
        <div class="col-md-6">
            <div class="img-container">
                <img src="data:image/png;base64," class="img-fluid rounded shadow-custom" style="width: 100%; height: 100%; object-fit: cover;">
            </div>
        </div>
        <div class="col-md-6 d-flex align-items-center">
            <div>
                <h2 class="text-dark-green">About ABC Retail</h2>
                <p class="text-dark-green">At ABC Retail, we are committed to providing the best products and services to our customers. Whether you're shopping for electronics, clothing, or home goods, we have it all. Our mission is to deliver quality products at competitive prices, with exceptional customer service.</p>
                <a href="#contact-us" class="btn btn-dark-green">Contact Us</a>
            </div>
        </div>
    </div>


    <!-- Contact Us Section -->
    <div id="contact-us" class="mt-5">
        <h2 class="text-center text-dark-green">Get in Touch</h2>
        <p class="text-center text-muted">We'd love to hear from you. Reach out to us for any queries or support.</p>
        <div class="row justify-content-center">
            <div class="col-md-6">
                <form>
                    <div class="form-group">
                        <label for="name" class="text-dark-green">Name</label>
                        <input type="text" class="form-control" id="name" placeholder="Enter your name">
                    </div>
                    <div class="form-group">
                        <label for="email" class="text-dark-green">Email</label>
                        <input type="email" class="form-control" id="email" placeholder="Enter your email">
                    </div>
                    <div class="form-group">
                        <label for="message" class="text-dark-green">Message</label>
                        <textarea class="form-control" id="message" rows="4" placeholder="Write your message"></textarea>
                    </div>
                    <button type="submit" class="btn btn-dark-green btn-block">Send Message</button>
                </form>
            </div>
        </div>
    </div>

    <!-- Footer -->
    <footer class="mt-5 py-4 bg-dark-green text-white text-center">
        <p>&copy; @DateTime.Now.Year ABC Retail. All rights reserved.</p>
        <p>Follow us on <a href="#" class="text-white">Facebook</a>, <a href="#" class="text-white">Instagram</a>, and <a href="#" class="text-white">Twitter</a>.</p>
    </footer>
</div>


